@using BestPlace.Core.Models.Call;
@model IEnumerable<CallListViewModel>
@{
    ViewData["Title"] = "All";
}
<div class="midde_cont">
                  <div class="container-fluid">
                     <div class="row column_title">
                        <div class="col-md-12">
                           <div class="page_title">
                              <h2>Tables</h2>
                           </div>
                        </div>
                     </div>
                     <!-- row -->
                     <div class="row">
                       
                        <!-- table section -->
                        <div class="col-md-12">
                           <div class="white_shd full margin_bottom_30">
                              <div class="full graph_head">
                                 <div class="heading1 margin_0">
                                    <h2>Item table</h2>
                                 </div>
                              </div>
                              <div class="table_section padding_infor_info">
                                 <div class="table-responsive-sm">
                                    <table class="table">
                                       <thead>
                                          <tr>
                                             <th>From</th>
                                          </tr>
                                       </thead>
                                       <tbody>
                                       @foreach ( var call in @Model)
                                       {
                                           <tr>
                                               <td><a asp-area="Admin" asp-controller="Call" asp-action="Details" asp-route-id="@call.Id"  > @call.Email</a> </td>
                                               <th><a asp-area="admin" asp-action="Remove" asp-controller="Call" asp-route-id ="@call.Id"> Remove</a></th>
                                           </tr>
                                       }
                                      
                                       </tbody>
                                    </table>
                                 </div>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
 </div>