@using  BestPlace.Core.Models.Category
@model CategoryAddViewModel
@{
    ViewData["Title"] = "Add";
}
 <div class="full_container">
         <div class="container">
            <div class="center verticle_center full_height">
               <div class="login_section">
                  <div class="logo_login">
                     <div class="center">
                        <img width="210" src="images/logo/logo.png" alt="#" />
                     </div>
                  </div>
                  <div class="login_form">
                      <form method="post" enctype="multipart/form-data">
">
                        <fieldset>
                           <div class="field">
                               <label class="label_field">Category name</label>
                              <input type="text" asp-for="Name" name="name" />
                           </div>
                           <div class="field">
                              <label class="label_field">Password</label>
                              <input type="file" asp-for="Image" name="Image" />
                           </div>
                            <div class="field margin_0">
                              <label class="label_field hidden">hidden label</label>
                              <button type="submit" value="Upload" class="main_bt">Add</button>
                           </div>
                        </fieldset>
                     </form>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <!-- jQuery -->
      <script src="~/Admin/js/jquery.min.js"></script>
      <script src="~/Admin/js/popper.min.js"></script>
      <script src="~/Admin/js/bootstrap.min.js"></script>
      <!-- wow animation -->
      <script src="~/Admin/js/animate.js"></script>
      <!-- select country -->
      <script src="~/Admin/js/bootstrap-select.js"></script>
      <!-- nice scrollbar -->
      <script src="~/Admin/js/perfect-scrollbar.min.js"></script>
      <script>
         var ps = new PerfectScrollbar('#sidebar');
      </script>
      <!-- custom js -->
      <script src="~/Admin/js/custom.js"></script>